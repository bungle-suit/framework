{
  "name": "bungle/framework",
  "description": "Bungle framework library",
  "type": "library",
  "license": "MIT",
  "require": {
    "doctrine/annotations": "^1.11",
    "symfony/dependency-injection": "^5.2",
    "symfony/config": "^5.2",
    "symfony/workflow": "^5.2",
    "symfony/security-core": "^5.2",
    "symfony/form": "^5.2",
    "symfony/yaml": "^5.2",
    "symfony/validator": "^5.2",
    "ext-json": "*",
    "symfony/string": "^5.2",
    "phpoffice/phpspreadsheet": "^1.16",
    "webmozart/assert": "^1.9",
    "aura/sqlquery": "^2.7"
  },
  "require-dev": {
    "pestphp/pest": "^1.0"
  },
  "autoload": {
    "psr-4": { "Bungle\\Framework\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Bungle\\Framework\\Tests\\": "tests/" }
  },
  "extra": {
    "phpstan": {
      "includes": [
        "extension.neon"
      ]
    }
  }
}
